{
    "name": "matthiasnoback/gherkin-features-html-exporter",
    "description": "A tool for exporting Gherkin features to HTML",
    "require": {
        "php": ">=7.4",
        "behat/gherkin": "^4.8",
        "symfony/console": "^5.2",
        "thecodingmachine/safe": "^1.3",
        "league/commonmark": "^1.5"
    },
    "require-dev": {
        "behat/behat": "^3.8",
        "phpunit/phpunit": "^9.5",
        "phpstan/phpstan": "^0.12.83"
    },
    "autoload": {
        "psr-4": {
            "GherkinHtmlExporter\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Test\\": "test/"
        }
    },
    "bin": [
        "bin/export.php"
    ]
}
